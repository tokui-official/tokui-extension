{
  "name": "tokui-extendsion",
  "description": "tokui extendsion",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "tokui-extendsion",
    "default_popup": "popup/popup.html"
